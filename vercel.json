{
    "version": 1,
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/index.html"
      }
    ]
  }
