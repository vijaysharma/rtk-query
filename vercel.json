{
    "version": 2,
    "builds": [
      {
        "src": "api/dist/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/dist/index.html"
      }
    ]
  }